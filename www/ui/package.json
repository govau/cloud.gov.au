    "@nivo/pie": "^0.33.0",
    "@types/google.analytics": "^0.0.37",
    "@types/jest": "22.1.2",
    "@types/node": "9.3.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-helmet": "^5.0.4",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-test-renderer": "^16.0.0",
    "core-js": "^2.5.5",
    "date-fns": "^1.29.0",
    "grid-styled": "^2.0.0-11",
    "jest-styled-components": "^4.10.0",
    "loadable-components": "^0.4.0",
    "lodash-es": "^4.17.5",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0",
    "react-snap": "^1.11.3",
    "react-test-renderer": "^16.2.0",
    "source-map-explorer": "^1.5.0",
    "styled-components": "2.4.0",
    "styled-normalize": "^4.0.0",
    "styled-system": "^1.1.7",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "typescript-styled-plugin": "^0.4.0",
    "whatwg-fetch": "^2.0.3"
  },
  "proxy": "http://localhost:8080",
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "resolutions": {
    "ws": "3.3.2"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "format": "prettier --write \"**/*.{js,json,jsx,md,ts,tsx}\"",
    "format-check": "prettier --list-different \"**/*.{js,json,jsx,md,ts,tsx}\"",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && react-snap",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  }
}
